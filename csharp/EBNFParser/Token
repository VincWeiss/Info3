using EbnfParsers;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace EbnfParser
{
    public class Token
    {
        public EBNFParser.Tokens TokenName { get; set; }

        public string TokenValue { get; set; }

        public Token(EBNFParser.Tokens name, string value)
        {
            TokenName = name;
            TokenValue = value;
        }
    }
}
